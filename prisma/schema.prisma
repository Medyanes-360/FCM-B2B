// Model Güncellendiğinde yapılacaklar:
// 1-" npx prisma generate " bunu terminale yaz (enter!)
// 2- VS Codu kapatıp yeniden aç.
// ! Sırayla yaparsan gücelleme gerçekleşir.

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model CARKART {
  CARKOD           String     @id
  CARBAKIYE        Float
  CARBORCTOP       Float
  CARALACAKTOP     Float
  CARYETKILI       String?
  CARUNVAN         String?
  CARUNVAN3        String?
  CAROZKOD1        String?
  CAROZKOD2        String?
  CAROZKOD3        String?
  CAROZKOD4        String?
  CAROZKOD5        String?
}

model STKKART {
  STKKOD           String     @id  
  STKCINSI         String?    
  STKBAKIYE        Float
  STKEKDATE1       DateTime?
  STKOZKOD1        String?
  STKOZKOD3        String? 
  STKOZKOD2        String? 
  STKOZKOD5        String? 
}

model CARHAR {
  CARHARCARKOD   String     @id
  CARHARREFNO    Int?
  CARHARTAR      DateTime?
  CARHARCARUNVAN String?
  CARHARISTIPKOD String?
  CARHARACIKLAMA String?
  CARHARTUTAR    Int?
  CARHARGCFLAG   Int?
  CARHARVADETAR  DateTime?
  CARHARACIKLAMA1 String?
}

model FATFIS {
  FATFISREFNO  Int         @id
  FATFISCARREFNO Int?

}


model FATHAR {
  FATHARCARKOD     String @id
  FATHARREFNO      Int?
  FATHARTAR DateTime?
  FATHARSTKKOD String?
  FATHARSTKCINS String?
  FATHARMIKTAR Int?
  FATHARTOPLAMTUT Float
  FATHARFIYAT Int?
  FATHARTUTAR Int?
}
